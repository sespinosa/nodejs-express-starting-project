{
  "name": "graphql-fed",
  "version": "1.0.0",
  "description": "A graphql federation layer to execute search flows with NLP.",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src --exec babel-node src/index.js",
    "build": "npm run lint && npm test && npm run unit-test && babel src --out-dir dist --source-maps --ignore 'src/**/*.spec.js'",
    "serve": "node dist/index.js",
    "test": "mocha --require @babel/register --exit",
    "unit-test": "mocha src/**/*.spec.js --require @babel/register",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "keywords": [
    "express",
    "nodejs"
  ],
  "author": "Sebasti√°n Espinosa <sebastian.espinosa@mcplusa.com>",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.11.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  }
}
